{
  "name": "quickmern-client",
  "version": "1.0.0",
  "description": "Quick way to start a project",
  "main": "index.js",
  "author": "Charlie Hay",
  "license": "UNLICENCED",
  "homepage": "https://github.com/haytherecharlie/quickmern",
  "scripts": {
    "build": "../node_modules/react-scripts/bin/react-scripts build",
    "coverage": "yarn test -- --coverage",
    "dev": "react-scripts start",
    "lint": "eslint ./src",
    "prod": "node build/index.js",
    "test": "react-scripts test --env=jsdom",
    "styleguide": "npx styleguidist server"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.2.2",
    "react-redux": "latest",
    "react-router-dom": "latest",
    "redux": "latest"
  },
  "devDependencies": {
    "react-scripts": "latest"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ],
    "snapshotSerializers": []
  }
}
